    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remark-smartypants": "^3.0.1",
    "responsive-loader": "^3.1.2",
    "sharp": "^0.33.3",
